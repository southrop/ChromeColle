{
  "manifest_version": 2,
  "name": "ChromeColle",
  "short_name": "ChromeColle",
  "description": "KanColle in your Chrome",
  "version": "0.0.1",
  "author": "southrop <southrop113@gmail.com>",
  "minimum_chrome_version": "38",
  "default_locale": "en",
  "offline_enabled": false,
  "permissions": [
    "webview",
    "<all_urls>"
  ],

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
